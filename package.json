{
  "name": "workstream",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace @workstream/backend start",
    "start:frontend": "yarn workspace @workstream/frontend start",
    "dev:backend": "yarn workspace @workstream/backend dev",
    "dev:frontend": "yarn workspace @workstream/frontend dev",
    "build": "yarn workspace @workstream/shared build && yarn workspaces run build",
    "build:backend": "yarn workspace @workstream/backend build",
    "build:frontend": "yarn workspace @workstream/frontend build",
    "test": "yarn workspaces run test"
  },
  "main": "index.js",
  "repository": "git@github.com:vasyl-solutn/workstream.git",
  "author": "vasyl-ws <vboychuk@glenflow.com>",
  "license": "MIT"
}
